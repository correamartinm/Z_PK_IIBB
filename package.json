{
    "name": "z_pk_iibb",
    "version": "0.0.1",
    "private": true,
    "sapuxLayer": "CUSTOMER_BASE",
    "description": "A Fiori application.",
    "keywords": [
        "ui5",
        "openui5",
        "sapui5"
    ],
    "main": "webapp/index.html",
    "dependencies": {},
    "devDependencies": {
        "@ui5/cli": "^2.14.1",
        "@sap/ux-ui5-tooling": "1",
        "rimraf": "^3.0.2",
        "ui5-task-flatten-library": "0.2.5"
    },
    "scripts": {
        "start": "fiori run --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#zpkiibbzpkiibb-display\"",
        "start-dist": "fiori run --open index.html --config ui5-dist.yaml",
        "start-local": "fiori run --open index.html --config ui5-local.yaml",
        "build": "ui5 build --config=ui5.yaml --clean-dest --dest dist",
        "deploy": "npm run build && fiori deploy --config ui5-deploy.yaml && rimraf archive.zip",
        "deploy-config": "fiori add deploy-config",
        "zip": "cd dist && npx bestzip ../app.zip *",
        "startok": "fiori run --open index.html",
        "undeploy": "fiori undeploy --config ui5-deploy.yaml",
        "deploy-test": "npm run build && fiori deploy --config ui5-deploy.yaml --testMode true"
    },
    "ui5": {
        "dependencies": [
            "@sap/ux-ui5-tooling",
            "ui5-task-flatten-library"
        ]
    }
}